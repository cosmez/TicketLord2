@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}
@if (Model.Tickets is not null)
{
    <div class="container">
        <div class="row">
            @foreach (var ticket in Model.Tickets)
            {
                string name = ticket.Descripcion;
                <div class="col-2" data-value="@(ticket.Descripcion + "")">@name</div>
            }
        </div>
    </div>
}